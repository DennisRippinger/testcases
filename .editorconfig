# EditorConfig is awesome: http://EditorConfig.org

root = true

[*]
max_line_length = 120
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 4
continuation_indent_size = 8

[*.yaml]
indent_size = 2
continuation_indent_size = 4

[*.properties]
charset = latin1

[*.{cmd,bat]]
end_of_line = crlf
